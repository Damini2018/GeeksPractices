import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		//code
	
	Scanner sc=new Scanner(System.in);
		int t= sc.nextInt();
		for(int i=0;i<t;i++){
			int n= sc.nextInt();
			int ar[]=new int[n];
		        for(int k=0;k<n;k++){
				ar[k]=sc.nextInt();}
				
		
			int max1=0, max=Integer.MIN_VALUE;
			 for(int k=0;k<n;k++){
			     max1=max1+ar[k];
			     if(max1<0)max1=0;
			     if(max<max1){
			         max=max1;
			     }
			 }
			 
			 System.out.println(max);
		}
		}
}